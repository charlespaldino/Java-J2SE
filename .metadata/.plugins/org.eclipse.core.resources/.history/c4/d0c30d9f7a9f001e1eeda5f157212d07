import CircuitBoards.BasicBoards.Addition8BitBoard;

public class Program {

	public static void main(String[] args) 
	{
		
		//Manual testing here
		
		Addition8BitBoard adder = new Addition8BitBoard();
		
		adder.getInputConnectors().get(0).setValue(false);
		adder.getInputConnectors().get(1).setValue(true);
		adder.getInputConnectors().get(2).setValue(true);
		adder.getInputConnectors().get(3).setValue(true);
		adder.getInputConnectors().get(4).setValue(true);
		adder.getInputConnectors().get(5).setValue(true);
		adder.getInputConnectors().get(6).setValue(true);
		adder.getInputConnectors().get(7).setValue(true);
		adder.getInputConnectors().get(8).setValue(true);
		adder.getInputConnectors().get(9).setValue(true);
		adder.getInputConnectors().get(10).setValue(true);
		adder.getInputConnectors().get(11).setValue(true);
		adder.getInputConnectors().get(12).setValue(true);
		adder.getInputConnectors().get(13).setValue(true);
		adder.getInputConnectors().get(14).setValue(true);
		adder.getInputConnectors().get(15).setValue(true);
		adder.getInputConnectors().get(16).setValue(true);
		
		for(int x = 0; x < 9; x++)
		{
			System.out.println("S"+(x+1)+": " + adder.getOutputConnectors().get(x).getValue());	
		}
		
		
	}

}
